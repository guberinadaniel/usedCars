        <p>By : {{ author_name }}</p>
        <p class="blog-category">Category : {{ content.field_blog_category }}</p>
        <p class="blog-text">{{ content.field_blog_text }}</p>
    </div>
<h3>Comments</h3>
{{ content.field_comment }}

