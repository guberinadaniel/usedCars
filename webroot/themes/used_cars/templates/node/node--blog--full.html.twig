{{ title_prefix }}
  {% if not page %}
      <h2{{ title_attributes }}>
          <a href="{{ url }}" rel="bookmark">{{ label }}</a>
      </h2>
  {% endif %}
  {{ title_suffix }}


<div class="used-cars-blog">
    <div class="used-cars-blog-wraper1">
        <div class="blog-image">{{ content.field_blog_image }}</div>
